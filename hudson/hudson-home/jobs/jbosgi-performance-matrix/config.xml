<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>&lt;h2&gt;Run performance tests across all supported frameworks&lt;/h2&gt;&#xd;
&#xd;
Latest &lt;a href=&apos;ws/testsuite/performance/target/performance-report.html&apos;&gt;Performance Results&lt;/a&gt;</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.security.AuthorizationMatrixProperty>
      <permission>hudson.model.Item.Read:anonymous</permission>
      <permission>hudson.model.Item.Workspace:anonymous</permission>
    </hudson.security.AuthorizationMatrixProperty>
    <com.coravy.hudson.plugins.github.GithubProjectProperty>
      <projectUrl>@scm.http.url.jbosgi@</projectUrl>
    </com.coravy.hudson.plugins.github.GithubProjectProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM">
    <configVersion>1</configVersion>
    <remoteRepositories>
      <org.spearce.jgit.transport.RemoteConfig>
        <string>origin</string>
        <int>5</int>
        <string>fetch</string>
        <string>+refs/heads/*:refs/remotes/origin/*</string>
        <string>receivepack</string>
        <string>git-upload-pack</string>
        <string>uploadpack</string>
        <string>git-upload-pack</string>
        <string>url</string>
        <string>@scm.git.url.jbosgi@</string>
        <string>tagopt</string>
        <string></string>
      </org.spearce.jgit.transport.RemoteConfig>
    </remoteRepositories>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <mergeOptions/>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <clean>false</clean>
    <choosingStrategy>Default</choosingStrategy>
    <submoduleCfg class="list"/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.TimerTrigger>
      <spec># Build at 3 am
0 3 * * *</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command># Update the Git submodules
if [ -f .gitmodules]; then
  git submodule init
  git submodule update
fi
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Maven>
      <targets>clean</targets>
      <mavenName>@maven.name@</mavenName>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Maven>
      <targets>-fae install</targets>
      <mavenName>@maven.name@</mavenName>
      <pom>testsuite/performance/pom.xml</pom>
      <properties>
      test=Service100TestCase,Service500TestCase,Service1000TestCase,Service5000TestCase,Bundle100TestCase,Bundle500TestCase,Bundle750TestCase
      bench=true
      framework=jboss
      </properties>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Maven>
      <targets>-fae install</targets>
      <mavenName>@maven.name@</mavenName>
      <pom>testsuite/performance/pom.xml</pom>
      <properties>
      test=Service100TestCase,Service500TestCase,Service1000TestCase,Service5000TestCase,Bundle100TestCase,Bundle500TestCase,Bundle750TestCase
      bench=true
      framework=felix
      </properties>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Maven>
      <targets>-fae install</targets>
      <mavenName>@maven.name@</mavenName>
      <pom>testsuite/performance/pom.xml</pom>
      <properties>
      test=Service100TestCase,Service500TestCase,Service1000TestCase,Service5000TestCase,Bundle100TestCase,Bundle500TestCase,Bundle750TestCase
      bench=true
      framework=equinox
      </properties>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
  </builders>
  <publishers>
    <hudson.tasks.junit.JUnitResultArchiver>
      <testResults>**/target/surefire-reports/TEST-*.xml</testResults>
      <testDataPublishers/>
    </hudson.tasks.junit.JUnitResultArchiver>
  </publishers>
  <buildWrappers/>
</project>
